{
    "name": "delaney/superban",
    "description": "Ban clients completely for a period of time, based on the number of requests made in a selected interval",
    "license": "MIT",
    "authors": [
        {
            "name": "Delaney Sylvans",
            "email": "delaneysylvans@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Delaney\\Superban\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/cache": "~8|^9.0|^10.0",
        "illuminate/http": "~8|^9.0|^10.0",
        "illuminate/routing": "~8|^9.0|^10.0",
        "illuminate/support": "~8|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "fossbarrow/laravel-phpcs": "dev-main"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Delaney\\Superban\\SuperbanServiceProvider"
            ]
        }
    }
}
